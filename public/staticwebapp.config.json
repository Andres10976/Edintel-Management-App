{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/static/*", "/*.{png,jpg,gif,svg,ico}"]
  },
  "mimeTypes": {
    ".ico": "image/x-icon",
    ".png": "image/png"
  },
  "routes": [
    {
      "route": "/logout",
      "rewrite": "/index.html",
      "allowedRoles": ["anonymous", "authenticated"]
    }
  ]
}